{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.eol": "\n",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.vscode-test/**": true,
    "**/.hg/store/**": true,
    "**/scripts/**": true
  },
  "search.exclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.vscode-test/**": true,
    "**/.hg/store/**": true,
    "**/scripts/**": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true
  }
}
